<Window x:Class="CPVLinker.Tools.HelpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CPVLinker.Tools"
        mc:Ignorable="d"
        Title="HelpWindow" Height="600" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="240"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="HelpList" Margin="10">
            <ListBoxItem Content="CSV 형식"/>
        </ListBox>
        <Image Grid.Column="1" Margin="10" Source="pack://siteoforigin:,,,/Resources/Basic 설명.png" />
    </Grid>
</Window>
